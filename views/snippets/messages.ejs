<main data-barba="chat-container" data-barba-namespace="chat">
    <div class="chat-input-wrapper">
        <button>Files</button>
        <input type="text" placeholder="<%= chat.publicInfo.name %> schreiben:" id="main-input" spellcheck="true">
        <button id="send">Send</button>
        <label for="image-file">Bild hochladen</label>
        <input style="display: none;" type="file" id="image-file" accept="image/png, image/jpg, image/gif, image/jpeg, image/tiff, image/raw, image/bpm, image/webp, image/ico">
        <button id="send-image">Bild senden</button>
        <label for="3d-file">3D-Datei hochladen</label>
        <input style="display: none;" type="file" id="3d-file" accept=".gltf,.glb">
        <button id="send-3d">3D-Datei senden</button>
    </div>
    <pre id="history"><%= chat.chatHistory %></pre>
    <pre id="symmetricKey"><%= chat.security.symmetricKey %></pre>
    <pre id="iv"><%= chat.security.iv %></pre>
</main>